{
  "name": "MyWeighBaby",
  "version": "1.2.2",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "postinstall": "patch-package",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "prettier": "prettier --write \"src/**/*.{js,jsx}\"",
    "prepare": "husky install",
    "clean": "react-native clean-project-auto",
    "generate-apk": "cd android && ./gradlew app:assembleRelease",
    "temp-release": "yarn generate-apk && yarn open-explorer",
    "open-explorer": "open android/app/build/outputs/apk/release",
    "bulldozer": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.24.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/picker": "1.8.1",
    "@react-navigation/native": "6.0.8",
    "@react-navigation/native-stack": "6.5.0",
    "esprima": "4.0.1",
    "husky": "7.0.1",
    "lodash-es": "4.17.15",
    "moment": "2.24.0",
    "patch-package": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-action-button": "2.8.5",
    "react-native-base64": "0.0.2",
    "react-native-ble-manager": "9.0.0",
    "react-native-ble-plx": "3.2.1",
    "react-native-bluetooth-classic": "1.60.0-rc.30",
    "react-native-bluetooth-state-manager": "^1.3.5",
    "react-native-canvas": "0.1.24",
    "react-native-elements": "0.19.1",
    "react-native-i18n": "2.0.15",
    "react-native-image-picker": "4.10.3",
    "react-native-linear-gradient": "2.5.3",
    "react-native-modal": "13.0.0",
    "react-native-modal-datetime-picker": "10.0.0",
    "react-native-permissions": "3.10.1",
    "react-native-picker-module": "2.0.4",
    "react-native-picker-select": "8.0.4",
    "react-native-reanimated": "2.14.4",
    "react-native-responsive-screen": "1.2.0",
    "react-native-root-toast": "3.2.1",
    "react-native-safe-area-context": "4.0.1",
    "react-native-safe-area-view": "1.1.1",
    "react-native-screens": "3.22.1",
    "react-native-share": "7.9.1",
    "react-native-svg": "12.1.1",
    "react-native-tts": "3.2.0",
    "react-native-uuid-generator": "6.1.1",
    "react-native-vector-icons": "7.0.0",
    "react-native-view-shot": "3.0.2",
    "react-native-webview": "^13.10.3",
    "react-use": "17.4.0",
    "scheduler": "0.15.0",
    "use-debounce": "8.0.1",
    "victory-native": "31.0.0"
  },
  "resolutions": {
    "stacktrace-parser": "0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "7.15.0",
    "@babel/plugin-proposal-decorators": "7.10.1",
    "@babel/runtime": "7.10.2",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "22.4.4",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-preset-react-native": "5.0.2",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "22.4.4",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "0.66.2",
    "prettier": "2.3.2",
    "react-native-clean-project": "3.6.7",
    "react-native-codegen": "0.0.8",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts",
      "./assets/app_icon",
      "./assets/social"
    ]
  }
}
